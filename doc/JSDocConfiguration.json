{
    "opts": {
        "destination": "doc/scrootinEyesJSDocs",
        "encoding": "utf8",
        "recurse": true,
        "template": "templates/default",
        "test": false,
        "tutorials": "examples"
    },
    "plugins": [],
    "recurseDepth": 10,
    "source": {
        "include": ["lib", "bin"],
        "exclude": ["node_modules"],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    }
}
